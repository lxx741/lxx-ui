    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.20",
    "@types/shelljs": "^0.8.15",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
