    "@lxx-ui/locale": "workspace:*",
    "@lxx-ui/hooks": "workspace:*",
    "@lxx-ui/theme": "workspace:*",
    "@lxx-ui/utils": "workspace:*",
    "lodash-es": "^4.17.21",
